
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title id="PageTitle">准备</title>
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Lib/m-Reset/m-Reset.css" rel="stylesheet" />
    <link href="~/Css/Base.css" rel="stylesheet" />
    <link href="~/Css/Paper/ExamIndex.css" rel="stylesheet" />
</head>
<body>
    <header>
        <h2 id="HeaderTitle">准备</h2>
    </header>
    <main>
        <form id="InputForm" autocomplete="off">
            <div class="form-group">
                <label for="SelectClass">选择班级</label>
                <select id="SelectClass" class="form-control"></select>
            </div>
            <div class="form-group">
                <label for="SelectPaper">选择试题</label>
                <select id="SelectPaper" class="form-control"></select>
            </div>
            <div id="ProblemCountGroup" class="form-group">
                <label for="InputStudentName">姓名</label>
                <input id="InputStudentName" type="text" class="form-control" required>
                <span class="help-block"></span>
            </div>
            <button id="BtnSubmit" type="button" class="btn btn-block btn-primary">确认</button>
        </form>
    </main>
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
    <script src="~/Lib/m-Tools/m-Tools.js"></script>
    <script src="~/Scripts/Base.js"></script>
    <script src="~/Scripts/Paper/ExamIndex.js"></script>
</body>
</html>